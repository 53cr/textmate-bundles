	cd "$TM_PROJECT_DIR/build/"
fi

htmlHeader "Building $TM_FLEX_FILE_SPECS (ant)";
importCSS "$TM_BUNDLE_SUPPORT/css/errors.css";

ant -find build.xml | parse_mxmlc_out.py</string>
	<key>input</key>
	<string>none</string>
	<key>keyEquivalent</key>
	<string>@B</string>
	<key>name</key>
