
<tr class="<%= $match[:type] %>">
	<td class="type"><%= $match[:type] %></td>
	<td class="file">
		<a title="<%= html_escape($match[:content].strip) %>" href="<%= TextMate.file_link($match[:file], $match[:line]) %>">
		<%= "#{html_escape(File.basename($match[:file]))}" %></a>
	</td>
	<td><%= $match[:line] %></td>
	<td class="comment"><%= $match[:match] %></td>
		
</tr>
